$('.withdraw_bid').unbind('ajax:success');
$('.withdraw_bid').bind('ajax:success', function(){
    var parent = $(this).parent();
    parent.empty();
    parent.append(
            "<%= escape_javascript(render :partial => "bids/withdrawnBid", :locals => {:bid => @bid}) %> "
    ).fadeIn();
    $("span.success").empty();
});